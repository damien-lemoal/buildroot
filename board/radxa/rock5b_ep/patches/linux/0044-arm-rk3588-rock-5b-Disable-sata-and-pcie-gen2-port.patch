From 4378401b292fdc52a360d7e25951e1a01aeebfd9 Mon Sep 17 00:00:00 2001
From: Damien Le Moal <dlemoal@kernel.org>
Date: Wed, 13 Dec 2023 16:11:31 +0900
Subject: [PATCH 44/47] arm: rk3588-rock-5b: Disable sata and pcie gen2 port

Signed-off-by: Damien Le Moal <dlemoal@kernel.org>
---
 arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts b/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts
index 059c552aa54d..6ac4185690b4 100644
--- a/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts
@@ -139,7 +139,7 @@ &combphy1_ps {
 };
 
 &sata1 {
-	status = "okay";
+	status = "disabled";
 };
 
 &cpu_b0 {
@@ -281,7 +281,7 @@ &pcie2x1l2 {
 	pinctrl-0 = <&pcie2_2_rst>;
 	reset-gpios = <&gpio3 RK_PB0 GPIO_ACTIVE_HIGH>;
 	vpcie3v3-supply = <&vcc3v3_pcie2x1l2>;
-	status = "okay";
+	status = "disabled";
 };
 
 &pcie30phy {
-- 
